{% extends "layout.njk" %}

{% block head %}
    <title>{{item.title}}</title>

    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
{% endblock %}

{% block content %}

    <div id="wrapper">
        <h1>{{item.title}}</h1>
    </div>

    <section class="cardPage">
        <div class="card" id="{{item.id}}">
            <div class="card__card-container">
                  <iframe src="partials/{{item.id}}" frameborder="0"></iframe>
                </div>
                <div class="card__content">
                    <p>{{item.title}}</p>
                </div>
            </div>

        </section>
    {% endblock %}